        android:strokeColor="?android:attr/textColorPrimary"
        android:strokeWidth="0.5"
        android:pathData="M 10,50 L 90,50" />
    <path
        android:strokeColor="?android:attr/textColorPrimary"
        android:strokeWidth="0.5"
        android:pathData="M 50,10 L 50,90" />
